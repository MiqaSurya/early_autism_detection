{
  "name": "early-autism-detector",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev",
    "dev:skip-checks": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:production": "cross-env NODE_ENV=production npm run build",
    "prebuild": "echo 'Skipping prebuild checks for development'",
    "postbuild": "echo 'Build completed successfully'",
    "clean": "rm -rf .next out",
    "vercel-build": "npm run build",
    "test": "echo 'Tests configured - run after production deployment'",
    "test:watch": "echo 'Test watch mode available'",
    "test:coverage": "echo 'Test coverage available'",
    "test:ci": "echo 'CI tests configured'",
    "validate:env": "node scripts/validate-production-env.js",
    "test:production": "node scripts/test-production-build.js",
    "test:build": "node scripts/test-build.js",
    "deploy:check": "npm run validate:env && npm run type-check && npm run lint && npm run build",
    "deploy:vercel": "npm run deploy:check && vercel --prod",
    "deploy:test": "npm run test:production && npm run deploy:vercel",
    "backup": "node scripts/supabase-backup.js",
    "backup:quick": "node scripts/supabase-backup.js --tables=profiles,children,assessments,autism_centers,center_users",
    "backup:schema": "node scripts/supabase-backup.js --schema-only",
    "backup:data": "node scripts/supabase-backup.js --data-only",
    "restore": "node scripts/supabase-restore.js",
    "restore:latest": "node scripts/supabase-restore.js --latest",
    "restore:dry-run": "node scripts/supabase-restore.js --dry-run --latest",
    "monitor": "node scripts/supabase-monitor.js",
    "monitor:health": "node scripts/supabase-monitor.js --check-health",
    "monitor:usage": "node scripts/supabase-monitor.js --usage-report",
    "monitor:continuous": "node scripts/supabase-monitor.js --continuous",
    "db:check": "npm run monitor:health",
    "db:backup": "npm run backup",
    "db:restore": "npm run restore:dry-run"
  },
  "dependencies": {
    "@geoapify/geocoder-autocomplete": "^2.1.0",
    "@hookform/resolvers": "^3.3.4",
    "@huggingface/inference": "^3.12.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.1.5",
    "@sendgrid/mail": "^8.1.5",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.39.3",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.0",
    "autoprefixer": "^10.4.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.10.5",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.312.0",
    "next": "^14.2.30",
    "openai": "^4.24.7",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "recharts": "^2.15.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.18",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.30",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^38.0.0"
  }
}
